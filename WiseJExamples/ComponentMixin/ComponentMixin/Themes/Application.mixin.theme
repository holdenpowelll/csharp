{
    "name": "Application.mixin",
    "images": {
        "my-icon-folder":
            "data:image/svg+xml;base64,",
        "my-icon-folder-open":
            "data:image/svg+xml;base64,",
        "my-icon-file":
            "data:image/svg+xml;base64,"
    },
    "appearances": {
        "my-list": {
            "text": "My ListBox",
            "states": {
                "default": {
                    "styles": {
                        "width": [1, 1, 1, 1],
                        "style": ["solid", "solid", "solid", "solid"],
                        "color": "windowFrame"
                    },
                    "properties": {
                        "backgroundColor": "window",
                        "opacity": 1,
                        "textColor": "windowFrame"
                    }
                },
                "focused": {
                    "styles": {
                        "color": "focusFrame"
                    }
                },
                "borderNone": {
                    "styles": {
                        "width": [0, 0, 0, 0]
                    }
                },
                "borderDashed": {
                    "styles": {
                        "width": [1, 1, 1, 1],
                        "style": ["dashed", "dashed", "dashed", "dashed"]
                    }
                },
                "borderDotted": {
                    "styles": {
                        "width": [1, 1, 1, 1],
                        "style": ["dotted", "dotted", "dotted", "dotted"]
                    }
                },
                "borderDouble": {
                    "styles": {
                        "width": 3,
                        "style": ["double", "double", "double", "double"]
                    }
                },
                "invalid": {
                    "styles": {
                        "color": "invalid",
                        "width": [1, 1, 1, 1]
                    }
                },
                "disabled": {
                    "properties": {
                        "opacity": 0.5
                    }
                }
            },
            "components": {
                "scrollbar-x": {
                    "text": "Scrollbar-X",
                    "inherit": "scrollbar"
                },
                "scrollbar-y": {
                    "text": "Scrollbar-Y",
                    "inherit": "scrollbar"
                },
                "tools": {
                    "states": {
                        "default": {
                            "styles": {
                                "color": "windowFrame",
                                "widthBottom": 1
                            }
                        }
                    }
                }
            }
        },
        "my-listitem": {
            "text": "My List Item",
            "states": {
                "default": {
                    "properties": {
                        "opacity": 1,
                        "padding": [4, 4, 6, 12],
                        "textColor": "inherit",
                        "backgroundColor": "inherit"
                    }
                },
                "hovered": {
                    "properties": {
                        "cursor": "pointer",
                        "textColor": "windowText",
                        "backgroundColor": "#BCD583"
                    }
                },
                "selected": {
                    "properties": {
                        "backgroundColor": "switchOn",
                        "textColor": "highlightText"
                    }
                },
                "disabled": {
                    "properties": {
                        "opacity": 0.5,
                        "cursor": "default"
                    }
                }
            },
            "components": {
                "checkbox": {
                    "inherit": "checkbox"
                },
                "icon": {
                    "inherit": "icon-dark"
                }
            }
        },
        "my-tree": {
            "text": "My Tree",
            "states": {
                "default": {
                    "styles": {
                        "width": [1, 1, 1, 1],
                        "style": ["solid", "solid", "solid", "solid"],
                        "color": "windowFrame"
                    },
                    "properties": {
                        "backgroundColor": "window",
                        "opacity": 1,
                        "textColor": "windowText"
                    }
                },
                "focused": {
                    "styles": {
                        "color": "focusFrame"
                    }
                },
                "disabled": {
                    "properties": {
                        "opacity": 0.5
                    }
                },
                "borderNone": {
                    "styles": {
                        "width": [0, 0, 0, 0]
                    }
                },
                "borderDashed": {
                    "styles": {
                        "width": [1, 1, 1, 1],
                        "style": ["dashed", "dashed", "dashed", "dashed"]
                    }
                },
                "borderDotted": {
                    "styles": {
                        "width": [1, 1, 1, 1],
                        "style": ["dotted", "dotted", "dotted", "dotted"]
                    }
                },
                "borderDouble": {
                    "styles": {
                        "width": [3, 3, 3, 3],
                        "style": ["double", "double", "double", "double"]
                    }
                }
            },
            "components": {
                "scrollbar-y": {
                    "text": "Scrollbar-Y",
                    "inherit": "scrollbar"
                },
                "scrollbar-x": {
                    "text": "Scrollbar-X",
                    "inherit": "scrollbar"
                },
                "editor": {
                    "inherit": "textfield"
                },
                "tools": {
                    "states": {
                        "default": {
                            "styles": {
                                "color": "windowFrame",
                                "widthBottom": 1
                            }
                        }
                    }
                }
            }
        },
        "mytree-file": {
            "text": "My Tree Item",
            "states": {
                "default": {
                    "properties": {
                        "textColor": "",
                        "indent": 20,
                        "icon": "my-icon-file",
                        "opacity": 1
                    },
                    "styles": {
                        "backgroundColor": null
                    }
                },
                "hovered": {
                    "styles": {
                        "backgroundColor": "#BCD583"
                    },
                    "properties": {
                        "cursor": "pointer"
                    }
                },
                "selected": {
                    "properties": {
                        "textColor": "highlightText"
                    },
                    "styles": {
                        "backgroundColor": "switchOn"
                    }
                },
                "disabled": {
                    "properties": {
                        "opacity": 0.5
                    }
                }
            },
            "components": {
                "checkbox": {
                    "inherit": "checkbox",
                    "states": {
                        "default": {
                        }
                    }
                },
                "state": {
                    "inherit": "icon-dark"
                },
                "icon": {
                    "inherit": "icon-dark"
                },
                "label": {
                    "text": "Label",
                    "states": {
                        "default": {
                            "properties": {
                                "marginLeft": 5,
                                "padding": [2, 5, 3, 0]
                            }
                        }
                    }
                }
            }
        },
        "mytree-folder": {
            "text": "My Tree Node",
            "components": {
                "open": {
                    "text": "Open",
                    "states": {
                        "default": {
                            "properties": {
                                "source": "node-closed"
                            },
                            "styles": {
                                "transition": "transform 250ms"
                            }
                        },
                        "opened": {
                            "properties": {
                                "source": "node-opened"
                            },
                            "styles": {
                                "transform": "rotateZ(45deg)"
                            }
                        },
                        "rightToLeft": {
                            "styles": {
                                "transform": "rotateZ(180deg)"
                            }
                        },
                        "opened-rightToLeft": {
                            "styles": {
                                "transform": "rotateZ(135deg)"
                            }
                        }
                    },
                    "inherit": "icon-dark"
                },
                "checkbox": {
                    "inherit": "checkbox",
                    "states": {
                        "default": {
                        }
                    }
                },
                "state": {
                    "inherit": "icon-dark"
                },
                "icon": {
                    "inherit": "icon-dark"
                },
                "label": {
                    "text": "Label",
                    "states": {
                        "default": {
                            "properties": {
                                "marginLeft": 5,
                                "padding": [2, 5, 3, 0]
                            }
                        }
                    }
                }
            },
            "states": {
                "default": {
                    "properties": {
                        "textColor": "",
                        "indent": 20,
                        "icon": "my-icon-folder",
                        "opacity": 1
                    },
                    "styles": {
                        "backgroundColor": null
                    }
                },
                "hovered": {
                    "styles": {
                        "backgroundColor": "#BCD583"
                    },
                    "properties": {
                        "cursor": "pointer"
                    }
                },
                "selected": {
                    "properties": {
                        "textColor": "highlightText"
                    },
                    "styles": {
                        "backgroundColor": "switchOn"
                    }
                },
                "opened": {
                    "properties": {
                        "icon": "my-icon-folder-open"
                    }
                },
                "disabled": {
                    "properties": {
                        "opacity": 0.5
                    }
                }
            }
        }
    }
}